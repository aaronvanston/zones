{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "plugins": [
    "jsx-a11y",
    "react",
    "import"
  ],
  "env": {
    "browser": true,
    "jest": true,
    "node": true
  },
  "rules": {
    "import/prefer-default-export": 0,
    "react/jsx-filename-extension": 0,
    "import/no-extraneous-dependencies": ["error", {
      "devDependencies": true
    }]
  }
}
