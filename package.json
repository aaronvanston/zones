{
  "name": "zones",
  "version": "0.1.0",
  "description": "A sample boilerplate project including React, Redux and Webpack",
  "main": "index.js",
  "scripts": {
    "start": "babel-node webpack/server.js",
    "build:prod": "yarn clean:dist && NODE_ENV=production webpack -p --config webpack/config.js",
    "test": "jest",
    "test:watch": "yarn test -- --watch",
    "lint": "yarn lint:js && yarn lint:style",
    "lint:js": "eslint --ignore-path .gitignore .",
    "lint:style": "stylelint 'src/**/*.css'",
    "clean:dist": "rimraf ./dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aaronvanston/zones.git"
  },
  "author": "Aaron Vanston @aaronvanston",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aaronvanston/zones/issues"
  },
  "homepage": "https://github.com/aaronvanston/zones#readme",
  "dependencies": {
    "normalize.css": "^7.0.0",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "redux": "^3.7.2"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.1.1",
    "babel-preset-airbnb": "^2.4.0",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "^0.28.4",
    "eslint": "^4.2.0",
    "eslint-config-airbnb": "^15.0.2",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.1.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.29.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^20.0.4",
    "postcss-cssnext": "^3.0.2",
    "postcss-import": "^10.0.0",
    "postcss-loader": "^2.0.6",
    "react-hot-loader": "^next",
    "style-loader": "^0.18.2",
    "stylelint": "^8.0.0",
    "stylelint-config-primer": "^2.0.0",
    "stylelint-order": "^0.6.0",
    "webpack": "^3.3.0",
    "webpack-dev-server": "^2.5.1"
  }
}
